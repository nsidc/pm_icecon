
#ss FC      = gfortran-mp-7
FC      = gfortran
FFLAGS  = -c -cpp -Wall -ffixed-form -ffixed-line-length-132 -fcheck=all

PGM     = boot_driver_sb2_np_nrt
OBJS    = boot_driver_sb2_np_nrt.o \
	  boot_ice_sb2_ssmi_np_nrt.o \
	  linfit.o ret_ic.o ret_linfit_v2.o ret_parameters_sb2.o \
	  clean_routines_sb2.o fix_output_gdprod.o \
	  ret_adj_adoff.o xfer_tbs_nrt.o ret_wtp.o ret_water_ssmi.o

$(PGM): $(OBJS)
	$(FC) -o $(PGM) $(OBJS)

.suffixes: .f .o
.f.o:
	$(FC) $(FFLAGS) $<

cleanobj:
	rm *.o



